<div class="card" style="width: 18rem;">
  <img class="card-img-top current-img" alt="Movie image" width="300px" height="300px">
  <div class="card-body">
    <p class="card-text current"></p>
    <p class="card-text current-revenue"></p>
  </div>
</div>

<p class="high-score"></p>
<p class="current-score"></p>

<div class="card" style="width: 18rem;">
  <img class="card-img-top next-img" alt="Movie image" width="300px" height="300px">
  <div class="card-body">
    <p class="card-text next"></p>
  </div>
  <button class="higher">HIGHER</button>
  <button class="lower">LOWER</button>
</div>

{{#if revenue}}
<script src="/js/revenue.js"></script>
{{/if}}
{{#if raiting}}
<script src="/js/raitings.js"></script>
{{/if}}